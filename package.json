{
  "name": "virality-analyzer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "setup": "node scripts/setup-supabase.js",
    "quick-start": "node scripts/quick-start.js",
    "db:setup": "npx supabase start && npx supabase db reset",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:restart": "npx supabase stop && npx supabase start",
    "db:reset": "npx supabase db reset",
    "db:migrate": "npx supabase migration new",
    "db:push": "npx supabase db push",
    "db:pull": "npx supabase db pull",
    "db:status": "npx supabase status",
    "db:studio": "npx supabase studio",
    "db:types": "npx supabase gen types typescript --local > types/database.ts",
    "db:seed": "npx supabase seed run",
    "postinstall": "echo 'Skipping db:types for deployment'"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
